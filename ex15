import math

raizA = 49 ** (1/2)
raizB = 8 ** (1/3)
raizC = 625 ** (1/4)
raizD = 1024 ** (1/10)
raizE = 81 ** (1/4)
raizF = 343 ** (1/3)

print("Exercício 15: ")

print(f"a) √49 = {raizA}")
print(f"a) √8 = {raizB}")
print(f"a) √625 = {raizC}")
print(f"a) √1024 = {raizD}")
print(f"a) √81 = {raizE}")
print(f"a) √343 = {raizF}")


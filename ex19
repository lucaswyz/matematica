import math

A = 196 ** (1/2)
B = 2 ** ((1/2)*2)
C = 8 ** (1/3)
D = 27 ** (1/3)
E = 625 ** (1/2)
F = 64 ** (1/(2*2*2))
G = 1/10000 ** (1/4)

print("Exercício 19")

print(f"a) √196 = {A}")
print(f"b) (√2)² = {B}")
print(f"c) ³√8 = {C}")
print(f"d) ³√27 = {D}")
print(f"e) √625 = {E}")
print(f"f) ²√²√²√64 = {F} ")
print(f"g) ⁴√1/10000 = {G}")
